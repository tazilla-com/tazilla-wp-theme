					"slug": "neutral-10"
				},
				{
					"color": "#F3F3F3",
					"name": "Neutral 11",
					"slug": "neutral-11"
				}
			]
		},
		"layout": {
			"contentSize": "645px",
			"wideSize": "1340px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "2X-Small",
					"size": "0.25rem",
					"slug": "4"
				},
				{
					"name": "X-Small",
					"size": "0.5rem",
					"slug": "8"
				},
				{
					"name": "Small",
					"size": "1rem",
					"slug": "16"
				},
				{
					"name": "Regular",
					"size": "1.5rem",
					"slug": "24"
				},
				{
					"name": "Large",
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "32"
				},
				{
					"name": "X-Large",
					"size": "clamp(2rem, 7vw, 3rem)",
					"slug": "48"
				},
				{
					"name": "2X-Large",
					"size": "clamp(3rem, 10vw, 4rem)",
					"slug": "64"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Unbounded",
							"fontStyle": "normal",
							"fontWeight": "200",
							"src": "file:./assets/fonts/unbounded/unbounded-200-normal.woff2"
						},
						{
							"fontFamily": "Unbounded",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": "file:./assets/fonts/unbounded/unbounded-300-normal.woff2"
						},
						{
							"fontFamily": "Unbounded",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": "file:./assets/fonts/unbounded/unbounded-400-normal.woff2"
						},
						{
							"fontFamily": "Unbounded",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": "file:./assets/fonts/unbounded/unbounded-500-normal.woff2"
						},
						{
							"fontFamily": "Unbounded",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": "file:./assets/fonts/unbounded/unbounded-600-normal.woff2"
						},
						{
							"fontFamily": "Unbounded",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": "file:./assets/fonts/unbounded/unbounded-700-normal.woff2"
						},
						{
							"fontFamily": "Unbounded",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": "file:./assets/fonts/unbounded/unbounded-800-normal.woff2"
						},
						{
							"fontFamily": "Unbounded",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": "file:./assets/fonts/unbounded/unbounded-800-normal.woff2"
						}
					],
					"fontFamily": "Unbounded, sans-serif",
					"name": "Unbounded",
					"slug": "unbounded"
				},
				{
					"fontFamily": "Arial, sans-serif",
					"name": "Arial",
					"slug": "arial"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "1rem"
					},
					"name": "Medium",
					"size": "1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.375rem",
						"min": "1.125rem"
					},
					"name": "Large",
					"size": "1.375rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2rem",
						"min": "1.75rem"
					},
					"name": "Extra Large",
					"size": "1.75rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "3rem",
						"min": "2.125rem"
					},
					"name": "Extra Extra Large",
					"size": "2.125rem",
					"slug": "xx-large"
				}
			],
			"writingMode": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var:preset|color|brand-light",
			"text": "var:preset|color|white"
		},
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var:preset|font-family|unbounded"
				}
			},
			"caption": {
				"typography": {
					"fontFamily": "var:preset|font-family|unbounded"
				}
			},
			"heading": {
				"color": {
					"text": "var:preset|color|brand-dark"
				},
				"typography": {
					"fontFamily": "var:preset|font-family|unbounded"
				}
			},
			"link": {
				"color": {
					"text": "var:preset|color--white"
				},
				"typography": {
					"fontFamily": "var:preset|font-family|unbounded"
				}
			}
		},
		"spacing": {
			"blockGap": "1rem",
			"padding": {
				"left": "var:preset|spacing|48",
				"right": "var:preset|spacing|48"
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|unbounded"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	]
}
