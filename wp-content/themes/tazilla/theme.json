				},
				{
					"color": "#71776C",
					"name": "Neutral 5",
					"slug": "neutral-5"
				},
				{
					"color": "#898E85",
					"name": "Neutral 6",
					"slug": "neutral-6"
				},
				{
					"color": "#A1A59D",
					"name": "Neutral 7",
					"slug": "neutral-7"
				},
				{
					"color": "#B8BBB5",
