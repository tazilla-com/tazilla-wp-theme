{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "tazilla/feature-button",
  "title": "Tazilla Feature Button",
  "category": "layout",
  "icon": "button",
  "editorScript": "file:./build/index.jsx.js",
  "style": "file:./build/style-style.scss.css",
  "ancestor": [
    "tazilla/features"
  ],
  "attributes": {
    "title": {
      "type": "string",
      "source": "html",
      "selector": "a",
      "default": "Feature"
    },
    "featureId": {
      "type": "string"
    },
    "activeFeatureId": {
      "type": "string"
    }
  }
}
