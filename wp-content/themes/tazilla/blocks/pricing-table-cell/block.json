{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "tazilla/pricing-table-cell",
  "title": "Cell",
  "parent": [
    "tazilla/pricing-table-row"
  ],
  "category": "layout",
  "icon": "table-col-after",
  "textdomain": "tazilla",
  "editorScript": "file:./build/index.jsx.js",
  "style": "file:./build/style-style.scss.css",
  "supports": {
    "html": false,
    "inserter": false
  },
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "span",
      "default": "--"
    },
    "hasCheckmark": {
      "type": "boolean",
      "default": false
    },
    "hasTooltip": {
      "type": "boolean",
      "default": false
    },
    "tooltipText": {
      "type": "string",
      "default": ""
    }
  }
}
